<link rel="stylesheet" href="/css/feedback-form.css">
<section class="feedback">
	<div class="feedback__wrapper">
		<h2 class="feedback__title">Список сообщений обратной связи</h2>
		<ul>
			<% feedbackList.forEach((feedback) => { _%>
				<li>
					<p>Имя клиента: <%= feedback.client_name -%></p>
					<p>Город/организация: <%= feedback.client_from -%></p>
					<p>Контактный номер телефона: <%= feedback.client_phone -%></p>
					<p>Контактный email: <%= feedback.client_email -%></p>
					<p>Сообщение: </p>
					<p><%= feedback.message -%></p>
				</li>
			<% }) _%>
		</ul>
	</div>
</section>
<script src="/js/feedback-form.js" defer></script>